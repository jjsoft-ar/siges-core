{
    "name": "jjsoft-ar/siges-core",
    "type": "library",
    "description": "Some core code for the SiGEs Platform",
    "keywords": [
        "JJSoft",
        "SigesCore"
    ],
    "homepage": "https://github.com/jjsoft-ar/siges-core",
    "license": "MIT",
    "authors": [
        {
            "name": "Jose C Gallo",
            "email": "aguara_gazu@yahoo.com.ar",
            "homepage": "http://aguaragazu.github.io/aguaragazu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "jjsoft-ar/siges-ui" : "^1.1",
        "pingpong/sky": "~2.1",
        "pingpong/modules": "~2.1",
        "pingpong/menus": "~2.1",
        "pingpong/widget" : "~2.1",
        "barryvdh/laravel-debugbar": "^2.1@dev",
        "styde/html": "~1.0",
        "joselfonseca/laravel-api-tools": "1.4.*",
        "joselfonseca/laravel-tactician" : "0.2.*",
        "joselfonseca/image-manager" : "2.1.*",
        "laracasts/utilities": "~2.0",
        "uxweb/sweet-alert": "~1.1",
        "yajra/laravel-datatables-oracle": "~6.0",
        "baum/baum": "~1.1",
        "cviebrock/eloquent-sluggable" : "3.1.*",
        "maatwebsite/excel": "~2.1.0",
        "venturecraft/revisionable": "1.*",
        "webpatser/laravel-uuid": "2.*",
        "prettus/l5-repository": "^2.1",
        "prettus/laravel-validation": "1.1.*",
        "fenos/notifynder": "^3.2",
        "elibyy/tcpdf-laravel": "5.1.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "orchestra/testbench": "3.2.*",
        "mockery/mockery": "0.9.*",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "JJSoft\\SigesCore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JJSoft\\SigesCore\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    }
}
